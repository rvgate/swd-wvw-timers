{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"SWD WvW Timers",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"SWD",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Demo App"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/main.html",
					"transparent":false,
					"resizable":true,
					"size":{"width":600, "height":300}
				},
			"SubWindow":{
					"file":"Files/notifications.html",
					"transparent":true,
					"resizable":false,
					"size":{"width":400, "height":300},
					"in_game_only":true,
                    "clickthrough": true
				}
		}
	}
}