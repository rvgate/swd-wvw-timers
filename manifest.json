{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"SWD WvW Timers",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"rvgate, dajs",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"SWD WvW Timers provides an in-game overlay displaying information about objectives being captured and/or claimed during a WvW match-up in Guild Wars 2. Including audiovisual notifications, border filtering and localization."
	},
	"data": {
		"start_window":"Main",
		"windows":{
			"Main":{
                "file":"Files/main.html",
                "transparent":false,
                "resizable":false,
                "size":{"width":500, "height":600}
            },
			"Notifications":{
                "file":"Files/notifications.html",
                "transparent": true,
                "resizable": false,
                "size":{"width":300, "height":500},
                "in_game_only": true,
                "clickthrough": true
            }
		}
	}
}